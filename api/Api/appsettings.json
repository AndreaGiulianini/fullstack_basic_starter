{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres_db;Database=your_db_name;Username=your_db_user;Password=your_db_password"
  },
  "Jwt": {
    "Secret": "your-super-secret-jwt-key-that-should-be-at-least-32-characters",
    "Issuer": "fullstack-api",
    "Audience": "fullstack-app"
  },
  "Redis": {
    "ConnectionString": "valkey:6379,password=your_valkey_password"
  },
  "Elasticsearch": {
    "Uri": "http://elasticsearch:9200"
  },
  "Cors": {
    "AllowedOrigins": "http://localhost:4200,http://localhost:80"
  }
}
